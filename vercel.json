{
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/sse",
      "dest": "api/index.py"
    },
    {
      "src": "/oauth2callback", 
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "excludeFiles": [
    "**/*.pyc",
    "**/__pycache__/**",
    ".env",
    ".git/**",
    ".venv/**",
    ".ruff_cache/**",
    "workspace_mcp.egg-info/**",
    "Dockerfile",
    "docker-compose.yml",
    "helm-chart/**",
    "mcp_server_debug.log",
    "manifest.json",
    "smithery.yaml",
    "uv.lock",
    "changes_since_120.txt"
  ]
}